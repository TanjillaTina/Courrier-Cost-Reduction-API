    <!-- Custom styles for this template 
   <link href="/css/jumbotron-narrow.css" rel="stylesheet">-->
  

   <link href="/css/cpr.css" rel="stylesheet">


   <!--Materialize starts here-->

      <!--Import Google Icon Font-->
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <!--Import materialize.css-->
      <link type="text/css" rel="stylesheet" href="/materialize/css/materialize.min.css"  media="screen,projection"/>
      

      <!--Let browser know website is optimized for mobile-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>


<!--Materialize ends here-->

 <!--Import jQuery before materialize.js-->
 <script type="text/javascript" src="/materialize/jque.min.js"></script>
 <script type="text/javascript" src="/materialize/js/materialize.min.js"></script>
 




    </head>
    <body>
          
      

   
      

            <div class="container-fluid fixed-top " style="background-color: #2d333a;">
           
                <nav>
                    <div class="nav-wrapper blue-grey darken-3">
                      <a href="#!" class="brand-logo"style="padding-left:60px; color:white;"><i class="material-icons prefix"style="color:white;">all_inbox</i>Courier Cost Reduction</a>
                      <ul class="right hide-on-med-and-down">
                        
                        <li><a href="/auth/logout"><i class="material-icons right">exit_to_app</i>Logout</a></li>
                      </ul>
        
                      <ul class="side-nav" id="mobile-demo">
                  
                          
                          <li><a href="/auth/logout"><i class="material-icons right">exit_to_app</i>Logout</a></li>
                     
                    </ul>
                    </div>
                  </nav>
            
          
               </div>
                      <!-- Fixed Top Menu div ends here -->
                      
          
      <br>
     
    
      <div class="row">
          <div class="col s0 m1"></div>
        <div class="col s12 m10">





            <div class="header">
                                    
                <h4 class="text-header">New Requests</h4>
                <div class="progress">
                        <div class="indeterminate"></div>
                    </div>
                          
            </div>


           
          <div class="card blue-grey darken-3">
                 
              <span class="card-title yellow-text text-accent-2">
                 
                  
                         
                  <div class="row">
                      <form class="col s12">
                        <div class="row">
                            
                          <div class="input-field col s12 yellow-text text-accent-2">
                            <i class="material-icons prefix md-48">search</i>
                            <input type="text" name="search" id="search" class="search">
                            <label class="yellow-text text-accent-2" for="icon_prefix">Searching Index</label>
                            
                          </div>
                        
                        </div>
                      </form>
                    </div>
       

                  </span>
          <div class="card-content yellow-text text-accent-2 blue-grey darken-2">
  
         
  
         <div style="overflow-x:auto;">
          <table id="userTbl" class="bordered centered responsive-table">
          <thead>
           
              <tr>
                <th colspan="6" class="ship">Shipper Or Shipping From</th>
                <th colspan="6" class="cosg">Cosignee</th>
                
                
              </tr>
           
            
              <tr>
                  <th class="ship">
                      <i class="material-icons prefix">person</i> Contact Person
                  </th>
                  
                  <th class="ship">
                      <i class="material-icons prefix">contact_phone</i>
                      Contact Person Number 
                  </th>
                  <th class="ship">
                      <i class="material-icons prefix">business</i>
                      Company Name
                  </th>
                  <th class="ship">
                      <i class="material-icons prefix">account_balance</i>
                      Company Address
                  </th>
                  <th class="ship">
                      <i class="material-icons prefix">location_city</i>
                      City
                  </th>
                  <th class="ship">
                      <i class="material-icons prefix">location_on</i>
                     Country
                  </th>
              
          


              <th class="cosg">
                  <i class="material-icons prefix">person</i>
                   Contact Person
              </th>
              <th class="cosg">
                  <i class="material-icons prefix">contact_phone</i>
                  Contact Person Number 
              </th>
              <th class="cosg">
                  <i class="material-icons prefix">business</i>
                  Company Name
              </th>
              <th class="cosg">
                  <i class="material-icons prefix">account_balance</i>
                  Company Address
              </th>
              <th class="cosg">
                  <i class="material-icons prefix">location_city</i>
                  City
              </th>
              <th class="cosg">
                  <i class="material-icons prefix">location_on</i>
                  Country
              </th>
              
              <th colspan="1" class="btr">
                  <i class="material-icons prefix">done_outline</i>
                Received Button
             </th>
              </tr>
              
          </thead>

            
              <% requests.forEach(function(requests){ %>
                <tr>
                    <td class="ship"><%= requests.shiperOrshippingTo[0].cpname1 %></td>
                    <td class="ship"><%= requests.shiperOrshippingTo[0].cpnum1 %></td>
                    <td class="ship"><%= requests.shiperOrshippingTo[0].comname1 %></td>
                    <td class="ship"><%= requests.shiperOrshippingTo[0].comadd1 %></td>
                    <td class="ship"><%= requests.shiperOrshippingTo[0].city1 %></td>
                    <td class="ship"><%= requests.shiperOrshippingTo[0].country1 %></td>
                    
                    
                    

                    <td class="cosg"><%= requests.conignee[0].cpname2 %></td>
                    <td class="cosg"><%= requests.conignee[0].cpnum2 %></td>
                    <td class="cosg"><%= requests.conignee[0].comname2 %></td>
                    <td class="cosg"><%= requests.conignee[0].comadd2 %></td>
                    <td class="cosg"><%= requests.conignee[0].city2 %></td>
                    <td class="cosg"><%= requests.conignee[0].country2 %></td>

                  <td class="btr">
                                        <!-- Modal Trigger -->
                      

                      <!-- Modal Structure -->
                      <a class="waves-effect waves-light btn modal-trigger" href="#<%=requests._id %>">Received</a>

                      <!-- Modal Structure -->
                      <div id="<%=requests._id %>" class="modal modal-fixed-footer blue-grey darken-3 yellow-text text-accent-2">
                        <div class="modal-content">


                          <h4>Details</h4>
                        
                          <!--   Showing Details Starts Here-->
                        <div class="row">

                          <div class="col s0 m6">
                              <ul class="collection with-header">
                                  <li class="collection-header"  style="background-color:#00796b ;">
                                      <% if (requests.reqtype=="INBOUND") { %>
                                        <h5>Shipping From</h5>
                                     <% } %>
                                    
                                     <% if (requests.reqtype=="OUTBOUND") { %>
                                        <h5>Shipper</h5>
                                     <% } %>
                                  </li>
                                  
                                  <li class="collection-item" style="background-color:#00796b ;"><div>Contact Person: <%= requests.shiperOrshippingTo[0].cpname1 %><a href="#!" class="secondary-content"><i class="material-icons prefix">person</i></a></div></li>
                                  <li class="collection-item" style="background-color:#00796b ;"><div> Contact Person Number: <%= requests.shiperOrshippingTo[0].cpnum1 %><a href="#!" class="secondary-content"><i class="material-icons prefix">contact_phone</i></a></div></li>
                                  <li class="collection-item" style="background-color:#00796b ;"><div>Company Name: <%= requests.shiperOrshippingTo[0].comname1 %><a href="#!" class="secondary-content"><i class="material-icons prefix">business</i></a></div></li>
                                  <li class="collection-item" style="background-color:#00796b ;"><div>Company Address: <%= requests.shiperOrshippingTo[0].comadd1 %><a href="#!" class="secondary-content"><i class="material-icons prefix">account_balance</i></a></div></li>
                                  <li class="collection-item" style="background-color:#00796b ;"><div>City: <%= requests.shiperOrshippingTo[0].city1 %><a href="#!" class="secondary-content"><i class="material-icons prefix">location_city</i></a></div></li>
                                  <li class="collection-item" style="background-color:#00796b ;"><div>Country: <%= requests.shiperOrshippingTo[0].country1 %><a href="#!" class="secondary-content"><i class="material-icons prefix">location_on</i></a></div></li>
                                </ul>
                                     
                          </div>
                          <div class="col s0 m6">
                              <ul class="collection with-header">
                                  <li class="collection-header" style="background-color:#00838f ;">
                                     
                                        <h5>Cosignee</h5>
                                    
                                  </li>
                                  
                                  <li class="collection-item" style="background-color:#00838f ;"><div>Contact Person: <%= requests.conignee[0].cpname2 %><a href="#!" class="secondary-content"><i class="material-icons prefix">person</i></a></div></li>
                                  <li class="collection-item" style="background-color:#00838f ;"><div> Contact Person Number: <%= requests.conignee[0].cpnum2 %><a href="#!" class="secondary-content"><i class="material-icons prefix">contact_phone</i></a></div></li>
                                  <li class="collection-item" style="background-color:#00838f ;"><div>Company Name: <%= requests.conignee[0].comname2 %><a href="#!" class="secondary-content"><i class="material-icons prefix">business</i></a></div></li>
                                  <li class="collection-item" style="background-color:#00838f ;"><div>Company Address: <%= requests.conignee[0].comadd2 %><a href="#!" class="secondary-content"><i class="material-icons prefix">account_balance</i></a></div></li>
                                  <li class="collection-item" style="background-color:#00838f ;"><div>City: <%= requests.conignee[0].city2 %><a href="#!" class="secondary-content"><i class="material-icons prefix">location_city</i></a></div></li>
                                  <li class="collection-item" style="background-color:#00838f ;"><div>Country: <%= requests.conignee[0].country2 %><a href="#!" class="secondary-content"><i class="material-icons prefix">location_on</i></a></div></li>
                                </ul>
                                     
                          </div>
                          
                          
                             
                          </div>

                          <!--   Showing Details Ends Here-->
                      

                          <!--   Form starts Here-->
                          <% if (requests.reqtype=="INBOUND") { %>
                            
                            <div class="row">
                                
                              <div class="col s0 m3"></div>
                              <div class="col s0 m6">
                               
                               
                                    <div class="card blue-grey darken-1">
                                      <div class="card-content white-text blue-grey darken-1">
                                        <span class="card-title"> <h5>Please, Enter The Cost and Press The Submit Button</h5></span>
                                        


                                        <form action="/confirms/SetForInbound" method="POST">
                                          <input type="hidden"  name="idi" value=<%=requests._id %> >
                                            <div class="row">
                                               
                                              <div class="input-field col s6">
                                                <i class="material-icons prefix">account_balance_wallet</i>
                                                <input id="icon_telephone" type="text" name="costi">
                                                <label for="icon_telephone">Cost</label>
                                              </div>
                                             
          
          
                                              <div class="input-field col s6">
                                                  <button class="btn waves-effect waves-light" type="submit" name="action">Submit
                                                      <i class="material-icons right">send</i>
                                                    </button>
                                                 
                                              </div>

                                             
          
          
          
                                            </div>
                                          </form>

                                      </div>
                                     
                                    </div>
                                  
                                </div>
                                <div class="col s0 m3"></div>
                              </div>
                                    
                           <% } %>
                           
                          <!--   Form  Ends Here-->
                          

                           <!--   Out Form  Starts Here-->
                           <% if (requests.reqtype=="OUTBOUND") { %>
                            
                            <div class="row">
                                
                                <div class="col s0 m3"></div>
                                <div class="col s0 m6">
                                 
                                 
                                      <div class="card blue-grey darken-1">
                                        <div class="card-content white-text blue-grey darken-1">
                                          <span class="card-title"> <h5>Please, Fill The Form and Press The Submit Button</h5></span>
                                          
  
  
                                          <form action="/confirms/SetForOutbound" method="POST">
                                             
                                            <input type="hidden"  name="ido" value=<%=requests._id %> >
                                              <div class="row">
                                                 
                                                <div class="input-field col s6">
                                                  <i class="material-icons prefix">credit_card</i>
                                                  <input id="icon_telephone" type="text" name="weight">
                                                  <label for="icon_telephone">Weight</label>
                                                </div>
                                               
            
            
                                                <div class="input-field col s6">
                                                    <i class="material-icons prefix">card_travel</i>
                                                    <input id="icon_telephone" type="text" name="boxsize">
                                                    <label for="icon_telephone">Box Size</label>
                                                  </div>
            
            
            
                                              </div>


                                              <div class="row">
                                                 
                                                  <div class="input-field col s6">
                                                    <i class="material-icons prefix">account_balance_wallet</i>
                                                    <input id="icon_telephone" type="text" name="costo">
                                                    <label for="icon_telephone">Cost</label>
                                                  </div>
                                                 
              
              
                                                  <div class="input-field col s6">
                                                      <button class="btn waves-effect waves-light" type="submit" name="action">Submit
                                                          <i class="material-icons right">send</i>
                                                        </button>
                                                  </div>
              
              
              
                                                </div>
                                            </form>
  
                                        </div>
                                       
                                      </div>
                                    
                                  </div>
                                  <div class="col s0 m3"></div>
                                </div>


                            <% } %>
                           <!--   Out Form  Ends Here-->




                        </div>
                          

                       
                        <div class="modal-footer">
                          <a href="#!" class="modal-action modal-close waves-effect waves-green btn-flat ">Agree</a>
                        </div>
                      </div>
                  

                  </td>
                </tr>
                <% }); %>
            
            </table>
            
            </div>


          </div>
         
      


        </div>
   



        </div>
        <div class="col s0 m1"></div>
      </div>
            
    
    
    

<script>
    $(document).ready(function(){
        $('.search').on('keyup',function(){
            var searchTerm = $(this).val().toLowerCase();
            $('#userTbl tbody tr').each(function(){
                var lineStr = $(this).text().toLowerCase();
                if(lineStr.indexOf(searchTerm) === -1){
                    $(this).hide();
                }else{
                    $(this).show();
                }
            });
        });
    });


    
  $(document).ready(function(){
    // the "href" attribute of the modal trigger must specify the modal ID that wants to be triggered
    $('.modal').modal();
  
  });
         
  </script>



                  

        
    </body>
</html>
