   <link href="/css/jumbotron-narrow.css" rel="stylesheet">-->
  

   <link href="/css/sccr.css" rel="stylesheet">


   <!--Materialize starts here-->

      <!--Import Google Icon Font-->
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <!--Import materialize.css-->
      <link type="text/css" rel="stylesheet" href="/materialize/css/materialize.min.css"  media="screen,projection"/>
      

      <!--Let browser know website is optimized for mobile-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>


<!--Materialize ends here-->

 <!--Import jQuery before materialize.js-->
 <script type="text/javascript" src="/materialize/jque.min.js"></script>
 <script type="text/javascript" src="/materialize/js/materialize.min.js"></script>
 




    </head>
    <body>
          
      

   
      

            <div class="container-fluid fixed-top " style="background-color: #2d333a;">
           
                <nav>
                    <div class="nav-wrapper blue-grey darken-3">
                      <a href="#!" class="brand-logo"style="padding-left:60px; color:white;"><i class="material-icons prefix"style="color:white;">all_inbox</i>Courier Cost Reduction</a>
                      <ul class="right hide-on-med-and-down">
                        <li><a href="/requests"><i class="material-icons left">cloud_download</i>Download Excell Sheet For New Requests</a></li>
                        <li><a href="/requests/SetCourierName"><i class="material-icons left">local_shipping</i>Set Courier Company Name of Downloaded Data</a></li>
                        <li><a href="/auth/logout"><i class="material-icons right">exit_to_app</i>Logout</a></li>
                      </ul>
        
                      <ul class="side-nav" id="mobile-demo">
                  
                          <li><a href="/requests"><i class="material-icons left">cloud_download</i>Download Excell Sheet For New Requests</a></li>
                          <li><a href="/requests/SetCourrierName"><i class="material-icons left">local_shipping</i>Set Courier Company Name of Downloaded Data</a></li>
                          <li><a href="/auth/logout"><i class="material-icons right">exit_to_app</i>Logout</a></li>
                     
                    </ul>
                    </div>
                  </nav>
            
          
               </div>
                      <!-- Fixed Top Menu div ends here -->
                      
          
      <br>
     
    
      <div class="row">
          <div class="col s0 m2"></div>
        <div class="col s12 m8">





            <div class="header">
                                    
                <h4 class="text-header">Set Courier Company Names of Downloaded Data</h4>
                <div class="progress">
                        <div class="indeterminate"></div>
                    </div>
                          
              </div>


    <% downsdate.forEach(function(downsdate){ %>
       
      <% downsdate.couns.forEach(function(couns){ %>
        
               

          <div class="card blue-grey darken-3">
                 
                <span class="card-title yellow-text text-accent-2">
                 
                <div class="row">
                  <h5 class="text-header col s6" style="padding-top:5px;">
                    Country Name :<%= couns.countryname %>
                  </h5>
                  <h5 class="text-header col s6" style="padding-top:5px;">
                      Download Date: <%=downsdate.downdate%>
                    </h5>

                    
                </div>
                       
                         <div class="row">
                                 
                                         <form class="col s12" action="/requests/InsertCourrierName" method="POST">
                                                 <div class="row">
                                                        <input type="hidden"  name="CounNamee" value=<%=couns.countryname %> >
                                                        <input type="hidden"  name="downdatee" value=<%=downsdate.downdate %> >
                                                   <div class="input-field col s6">
     
                                                     
                                                       <div class="row">
                                                           <div class="input-field">
                                                                     <i class="material-icons prefix">local_shipping</i>
                                                                     <input id="icon_telephone" type="text" class="validate" name="CComName">
                                                                     <label for="icon_telephone">Curier Company Name</label>
                                                           </div>
                                                       </div>
     
                 
     
                                                   </div>
                                                   <div class="col s6">
                                                     <br>
                                                       <button class="btn waves-effect waves-light" type="submit" name="action">Set Corier Company Name
                                                           <i class="material-icons right">send</i>
                                                         </button> 
     
     
                                                   </div>
                                                 </div>
                                                 
                                                 
                                         </form>
     
                             
       
                       </div>
     
  
                </span>
            <div class="card-content white-text blue-grey darken-2">
    
                
              <div class="row">
        
                    <div class="card">
                            
                             
                          
                      
                
                    

                        <ul class="collapsible" data-collapsible="accordion">
                            <% requests.forEach(function(requests){ %>

                              <% if (requests.shiperOrshippingTo[0].country1==couns.countryname && requests.downloaddate==downsdate.downdate) { %>
                           

                            <li>
                                <div class="collapsible-header blue-grey darken-3" style="color:white;">

                                        <% if (requests.reqtype=="INBOUND") { %>
                                          <i class="material-icons">arrow_downward</i>
                                        <% } %>

                                        <% if (requests.reqtype=="OUTBOUND") { %>
                                            <i class="material-icons">arrow_upward</i>
                                        <% } %>
                        
                                        <h6>Request By : <%=requests.userDetail[0].usermail%></h6>
                                      
                                        <hr>
                                        <h6>Request Date : <%=requests.userDetail[0].reqDate%></h6>


                                    <span class="new badge"><%= requests.reqtype %></span>
                              </div>



                              <div class="collapsible-body">
                              <table class="table bordered striped centered responsive-table">
                                  <thead>
                                  <tr>
                                      <th> </th>
                                      <% if (requests.reqtype=="INBOUND") { %>
                                          <th>Shipping From</th>
                                       <% } %>
                                      
                                       <% if (requests.reqtype=="OUTBOUND") { %>
                                          <th>Shipper</th>
                                       <% } %>
                                      
                                      <th>Cosignee</th>
                                  </tr>
                                  </thead>
                                  <tbody>
                                  <tr>
                                      <th>
                                          <i class="material-icons prefix">person</i> Contact Person
                                      </th>
                                      <td class="pt2"><%= requests.shiperOrshippingTo[0].cpname1 %></td>
                                      <td class="pt2"><%= requests.conignee[0].cpname2 %></td>
                                  </tr>
                                  <tr>
                                      <th>
                                              <i class="material-icons prefix">contact_phone</i>
                                              Contact Person Number 
                                      </th>
                                      <td><%= requests.shiperOrshippingTo[0].cpnum1 %></td>
                                      <td><%= requests.conignee[0].cpnum2 %></td>
                                  </tr>
                                  <tr>
                                      <th>
                                              <i class="material-icons prefix">business</i>
                                              Company Name
                                      </th>
                                      <td class="pt2"><%= requests.shiperOrshippingTo[0].comname1 %></td>
                                      <td class="pt2"><%= requests.conignee[0].comname2 %></td>
                                  </tr>
                                  <tr>
                                      <th>
                                              <i class="material-icons prefix">account_balance</i>
                                              Company Address
                                      </th>
                                      <td><%= requests.shiperOrshippingTo[0].comadd1 %></td>
                                      <td><%= requests.conignee[0].comadd2 %></td>
                                  </tr>
                                  <tr>
                                      <th>
                                              <i class="material-icons prefix">location_city</i>
                                              City
                                      </th>
                                      <td class="pt2"><%= requests.shiperOrshippingTo[0].city1 %></td>
                                      <td class="pt2"><%= requests.conignee[0].city2 %></td>
                                  </tr>
                                  <tr>
                                      <th>
                                              <i class="material-icons prefix">location_on</i>
                                              Country
                                      </th>
                                      <td><%= requests.shiperOrshippingTo[0].country1 %></td>
                                      <td><%= requests.conignee[0].country2 %></td>
                                  </tr>
                                  </tbody>
                              </table>
                            </div>
                            </li>



                            <% } %>

                            <% }); %>
                           
                          </ul>   
                           
                    </div>
      
    
            
              </div>
    
              
            </div>
           
          



            <div class="card-action">

                <form action="/requests/DownloadExcell2" method="POST">
                  <input type="hidden"  name="CounName" value=<%=couns.countryname %> >
                  <input type="hidden"  name="downdate" value=<%=downsdate.downdate %> >
                               
                              

                              <button class="btn waves-effect waves-ligh" type="submit">
                                 
                                              ReDownload Excell Sheet For This Country
                                              <i class="large material-icons">cloud_download</i>
                                        
                              </button>
                                          
                          
                </form>
            </div>



          </div>

          <% }); %>
    <% }); %>







        </div>
        <div class="col s0 m2"></div>
      </div>
            
    
    
    





<script>


  </script>



                  

        
    </body>
</html>
