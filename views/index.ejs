<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Courier Cost Reduction</title>
     
        
    <!-- Bootstrap
    <link href="/bootstrap/css/bootstrap.min.css" rel="stylesheet">

     -->
    <!-- Custom styles for this template 
   <link href="/css/jumbotron-narrow.css" rel="stylesheet"> -->
   <link href="/css/indexr.css" rel="stylesheet">


   <!--Materialize starts here-->

      <!--Import Google Icon Font-->
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <!--Import materialize.css-->
      <link type="text/css" rel="stylesheet" href="/materialize/css/materialize.min.css"  media="screen,projection"/>
      

      <!--Let browser know website is optimized for mobile-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>


<!--Materialize ends here-->


      <!--Import jQuery before materialize.js-->
      <script type="text/javascript" src="/materialize/jque.min.js"></script>
      <script type="text/javascript" src="/materialize/js/materialize.min.js"></script>
      

    


   
    </head>
    <body>
  

            <div class="row btag">
                

                <div class="col s0 m4 l3"></div>
                <div class="col s12 m4 l6">
                                <div class="header">
                        
                                    <h3 class="text-header">Courier Cost Reduction</h3>
                                  </div>
    
                                  <% if(mesg.error_msg) { %>
                    
                                       <script>
                                    Materialize.toast('<%= mesg.error_msg %>', 4000,'red accent-3');
                                    </script>
                                 <% }%>
                                 
                                              
                  <ul class="collapsible popout" data-collapsible="accordion">
                      <li>
          
                          <div class="collapsible-header blue-grey darken-3" style="color:white; padding-left: 45px;"> 
                              
                              <h5> To Submit Request Form Login with Office Mail </h5>
                               
                             </div>
                          <div class="collapsible-body darken-3-text blue-grey darken-3" >
          
                             <div class="row">
                                  <div class="col s12 m12">
                                      <div class="card-content">
                                      
                                              
                                          <div class="row">
                                             
                                              <a href="/auth/outlook" class="waves-effect waves-light btn-large btnlarge">
                                                <h5>
                                                    <i class=" large material-icons">mail</i>
                                                    
                                               Outlook +  </h5>
                                              </a>
                                          </div>
                                          
          
                                       
                                          
                                      </div>
                                   </div>
                              </div>



                          </div>
                      </li>
                              <li>
                                  <div class="collapsible-header blue-grey darken-3" style="color:white;padding-left: 170px;"> 
                              
                                      <h5> User Login For Admins </h5>
                                       
                                  </div>
                                <div class="collapsible-body  blue-grey darken-3 darken-3-text">


                                    <div class="card-content">
                                        
                                     
                                                <div class="row yellow-text  blue-grey darken-3">
                                                  <div class="col s2"></div>
                                                    <form  method="POST" action="/auth/login" class="col s8">
                                                      <div class="row">
                                                        <div class="input-field yellow-text">
                                                              <i class="material-icons prefix">perm_identity</i>
                                                              <input id="icon_telephone" type="text" class="validate" name="username">
                                                              <label for="icon_telephone" class="yellow-text">Username</label>
                                                        </div>
                                                        </div>
                                                 
                    
                                                        <div class="row yellow-text">
                                                        <div class="input-field yellow-text">
                                                                  <i class="material-icons prefix">security</i>
                                                                  <input id="icon_telephone" type="password" class="validate" name="password">
                                                                  <label for="icon_telephone" class="yellow-text">Password</label>
                                                        </div>
                                                        </div>
                                                        <button class="btn waves-effect waves-light btn-large" type="submit">Login
                                                          <i class="material-icons right">send</i>
                                                        </button>  
                                                    </form>
                                                    <div class="col s2"></div>
                                                  </div>
                                            
                                         
                                               

                                    </div>
                                </div>
                              </li>


                              <li>
                                    <div class="collapsible-header blue-grey darken-3" style="color:white;padding-left: 240px;"> 
                                
                                        <h5> View Chart </h5>
                                         
                                    </div>
                                  <div class="collapsible-body  blue-grey darken-3 darken-3-text">
  
  
                                      <div class="card-content">
                                          
                                       
                                                  <div class="row teal-text  blue-grey darken-3">
                                                    <div class="col s2"></div>
                                                      <form  method="POST" action="/viewChart" class="col s8">
                                                        <div class="row">
                                                          
                                                                <label><h5> Select Year</h5></label>
                                                                <select class="browser-default" name="selectyear">
                                                                  <option value="" disabled selected>Choose your option</option>
                                                                  <option value="2018"> 2018 </option>
                                                               
                                                                </select>
                                                                    
                                                         
                                                          </div>
                                                   
                      
                                                          
                                                          <button class="btn waves-effect waves-light btn-large" type="submit"> View Chart
                                                            <i class="material-icons right">search</i>
                                                          </button>  
                                                      </form>
                                                      <div class="col s2"></div>
                                                    </div>
                                              
                                           
                                                 
  
                                      </div>
                                  </div>
                                </li>
                            </ul>
                  
                   
          
          
               
               </div>
               <div class="col s0 m4 l3"></div>
              </div>






       
    </body>
</html>